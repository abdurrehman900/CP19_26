#01
def shape():
    n=int(input("enter number:"))
    a=1
    for i in range(1,n+1):
        b=1
        while(b<(n+1)-a):
            print("  ",end='')
            b+=1
        c=1
        while(c<a*2):
            if (c%2!=0):
                print(" 5 ", end='')
            else:
                print(" 2 ", end='')
            c+=1
        a+=1
        print("")
shape()
